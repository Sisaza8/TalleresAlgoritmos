Algoritmo ejercicio9
	Escribir "Inserte el sueldo del vendedor";
	Leer d1;
	Escribir "Inserte el valor de la primera venta";
	Leer d2;
	Escribir "Inserte el valor de la segunda venta";
	Leer d3;
	Escribir "Inserte el valor de la tercera venta";
	Leer d4;
	tv<-d2+d3+d4
	c<-tv*0.1
	pf<-d1+c
	Escribir "El valor total de las ventas es igual: " ,tv;
	Escribir "El valor de las comisiones corresponde a: " ,c;
	Escribir "El pago total es: " ,pf;
FinAlgoritmo
